# Example environment variables for deployment
# Copy this to `.env.local` for local development and fill real secrets.

# URL (relative path is fine when served from the same site)
MODEL_URL=/web_model/model.json

# If your frontend calls an external API, set its base URL
API_BASE_URL=https://api.example.com

# Secret API key (mark as secret in Vercel, do NOT commit real keys)
API_KEY=your_api_key_here

# Node environment
NODE_ENV=production

# Optional absolute public URL (useful for absolute links)
PUBLIC_URL=https://your-site.vercel.app

# Feature toggles
ENABLE_ANALYTICS=false

# Optional Sentry DSN (leave empty if not used)
SENTRY_DSN=
